{
  "name": "yannn/php-strict-data",
  "description": "PHP class for store data with check values based on annotation in PHPDoc.",
  "keywords": [
    "storage",
    "strict",
    "PHPDoc",
    "validate",
    "property",
    "type hinting",
    "enum",
    "annotation"
  ],
  "homepage": "https://github.com/yannn/php-strict-data",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Yaroslav Annenkov",
      "email": "ya@annenkov.ru",
      "homepage": "http://ya.annenkov.ru"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "PhpStrictData\\": "PhpStrictData/",
      "PhpStrictData\\Examples\\":   "examples/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpStrictData\\Tests\\": "tests/"
    }
  }
}
